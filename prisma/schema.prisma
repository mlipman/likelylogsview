generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DEV_DB_URL")
}

model Person {
  id Int @id @default(autoincrement())
  name String
  age Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Log {
  id Int @id @default(autoincrement())
  contents String
  pic1 String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
